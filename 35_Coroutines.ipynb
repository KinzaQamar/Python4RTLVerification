    "\n",
    "async def clk():\n",
    "    time.sleep(0.1)\n",
    "\n",
    "async def process():\n",
    "    for _ in range(2):\n",
    "        await clk();\n",
    "        time.sleep(0.2)\n",
    "        print(\"I am DONE waiting!\")\n",
    "\n",
    "await process()\n"
